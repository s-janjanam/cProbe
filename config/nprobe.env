# Basic Configuration
NPROBE_INTERFACE=enp104s0f0np0
NPROBE_INSTANCES=1
NPROBE_RSS_QUEUES=4

# PF_RING Configuration
NPROBE_USE_PFRING=true
NPROBE_DRIVER=i40e
NPROBE_ENABLE_ZC=true

# Flow Export
NPROBE_COLLECTOR=192.168.1.100:2055
NPROBE_TEMPLATE="%IPV4_SRC_ADDR %IPV4_DST_ADDR %IN_BYTES %OUT_BYTES"
NPROBE_VERBOSE_LEVEL=2

# Additional Options
NPROBE_EXTRA_OPTS="--flow-version 9"
